var GD = {tutorial : undefined,part : undefined,category : undefined,page : undefined,mobile : undefined,fixedWindow : undefined,updatePageInfo : undefined,setFixedWindow : undefined,mainMenu : undefined,testerMenu : undefined,reportBug : undefined,markMessage : undefined,hiddenExample : undefined,setContextPlusWindow : undefined,contextPlus : undefined,loadLinkContent : undefined,loadCSSFunction : undefined,partTooltip : undefined,colorCode : undefined,hashFix : undefined,adsense : undefined};GD.updatePageInfo = function(){var href = window.location.pathname;var href = href.slice(1);var href = href.split('/', 4);var mobile = $('#page').attr('data-mobile');this.tutorial = href[0];this.part = href[1];this.category = href[2];this.page = href[3];if(mobile == 'true'){this.mobile = true;}else{this.mobile = false;}};GD.setFixedWindow = function(){var root = $(':root');var body = $('body');body.append('<ins id="fixedWindow"><ins id="fixedWindowCloseButton">X</ins></ins>');this.fixedWindow = body.children(':last');this.fixedWindow.children(':first').click(function(){GD.fixedWindow.children(':gt(0)').hide();GD.fixedWindow.hide();root.css('overflow', 'auto');});$(document).keyup(function(event){if(event.keyCode == 27){if(GD.fixedWindow.is(':visible')){GD.fixedWindow.children(':gt(0)').hide();GD.fixedWindow.hide();root.css('overflow', 'auto');}}});};GD.mainMenu = function(){var placeholder = $('#page-header');var root = $(':root');setButtonMechanism();function setButtonMechanism(){placeholder.append('<ins id="mainMenuButton"><img src="/img/main-menu.png" alt="M"></ins>');placeholder.children(':last').one('click', function(){root.css('overflow', 'hidden');GD.fixedWindow.children(':gt(0)').hide();GD.fixedWindow.show();GD.fixedWindow.append('<ins id="mainMenuStructure"><samp>wczytujƒô...</samp></ins>');var structure = GD.fixedWindow.children(':last');var result = $.get('/gp-mechanism', 'type=getme&option=ajax&dir=all&file=main-menu');result.done(function(php){structure.html(php);setNavigationMechanism(structure);});result.fail(function(){structure.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});setShowHideMechanism($(this), structure);});};function setNavigationMechanism(structure){structure.find('section:first-of-type').addClass('on');structure.find('[class^=mainMenuLv] > ol > li:first-child').addClass('on');structure.find('[class^=mainMenuLv] > ol > li').click(function(){if(!$(this).hasClass('on')){$(this).parent().find('.on:first').removeClass('on');$(this).addClass('on');$(this).parent().parent().children('.on:first').removeClass('on');$(this).parent().parent().children(':eq(' + ($(this).index() + 1) + ')').addClass('on');}});};function setShowHideMechanism(button, structure){button.click(function(){if(structure.is(':hidden')){GD.fixedWindow.children(':gt(0)').hide();structure.show();GD.fixedWindow.show();root.css('overflow', 'hidden');}});};};GD.testerMenu = function(){var placeholder = $('#page-header');var root = $(':root');setButtonMechanism();function setButtonMechanism(){placeholder.append('<ins id="testerMenuButton"><img src="/img/tester-menu.png" alt="T"></ins>');placeholder.children(':last').one('click', function(){root.css('overflow', 'hidden');GD.fixedWindow.children(':gt(0)').hide();GD.fixedWindow.show();GD.fixedWindow.append('<ins id="testerMenuStructure"><samp>wczytujƒô...</samp></ins>');var structure = GD.fixedWindow.children(':last');var result = $.get('/gp-mechanism', 'type=getme&option=ajax&dir=all&file=tester-menu');result.done(function(php){structure.html(php);setNavigationMechanism(structure);});result.fail(function(){structure.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});setShowHideMechanism($(this), structure);});};function setNavigationMechanism(structure){var ol = structure.find('ol');if(GD['tutorial'] == 'kurs-tester' && GD['category'] == 'testowanie-wlasciwosci' || GD['tutorial'] == 'kurs-css' && GD['part'] == 'wlasciwosci'){var link = structure.find('a[href$="/' + GD['page'] + '"]:first');var section = link.parent().parent().parent();var index = section.index();link.addClass('on');section.parent().children(':gt(0)').hide();section.show();onLink(section, index);}else{var sectionHide = structure.find('section');var sectionShow = structure.find('section:first-of-type');sectionHide.hide();sectionShow.show();ol.children(':first-child').addClass('on');}ol.children().click(function(){if(!$(this).hasClass('on')){$(this).parent().children('.on:first').removeClass('on');$(this).addClass('on');$(this).parent().parent().children(':gt(0)').hide();$(this).parent().parent().children(':eq(' + ($(this).index() + 1) + ')').show();}});function onLink(section, index){var section = section.parent();if(!section.is('aside')){section.children(':first').children(':eq(' + (index - 1) + ')').addClass('on');onLink(section, section.index());}};};function setShowHideMechanism(button, structure){button.click(function(){if(structure.is(':hidden')){GD.fixedWindow.children(':gt(0)').hide();structure.show();GD.fixedWindow.show();root.css('overflow', 'hidden');}});};};GD.reportBug = function(){var placeholder = $('[data-report-bug]:first');if(placeholder.length){var process = placeholder.attr('data-report-bug');if(process == 'true'){var structure;var limit = 5000;var answer;var answerTarget;addButton();addClickEvent();   }}function addButton(){placeholder.after('<ins id="reportBug"><aside><h4>zg≈Ço≈õ b≈ÇƒÖd lub sugestiƒô</h4></aside></ins>');};function addClickEvent(){placeholder.next().children(':first').children(':first').one('click', function(){var h4Button = $(this);h4Button.after('<ins id="reportBugStructure"><samp>wczytujƒô...</samp></ins>');structure = h4Button.next();var result = $.get('/gp-mechanism', 'type=getme&option=ajax&dir=all&file=report-bug-form');result.done(function(php){structure.html(php);addTextLimit();addQuestion();submitBug();});result.fail(function(){structure.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});showHideContent(h4Button, structure);});};function addTextLimit(){var textarea = structure.find('textarea:first');textarea.after('<small>Pozosta≈Ço znak√≥w: <span>' + limit + '</span></small>');var warning = textarea.parent().children().eq(1).children().eq(0);textarea.keyup(function(){warning.text(limit - $(this).val().length);});};function addQuestion(){var h6 = structure.find('h6:last');h6.append(': <small></small>');answerTarget = h6.children().eq(0);refreshQuestion();};function refreshQuestion(){var l1 = parseInt(Math.random() * 10 + 1);var l2 = parseInt(Math.random() * 10 + 1);answer = l1 + l2;answerTarget.text(l1 + ' + ' + l2 + ' = ?');};function submitBug(){var form = structure.find('form');var sections = form.find('section');var userContent = sections.eq(0).find('textarea:first');var userContact = sections.eq(1).find('input:first');var userAnswer = sections.eq(2).find('input:first');var emptyWarning = 'TO POLE NIE MO≈ªE POZOSTAƒÜ PUSTE!!!';var answerWarning = 'B≈ÅƒòDNA ODPOWIED≈π!!!';var permission = true;userContent.add(userAnswer).focus(function(){if($(this).hasClass('reportBugWarning')){$(this).val('').removeClass('reportBugWarning');}});form.submit(function(event){event.preventDefault();var userContentValue = userContent.val();var userContactValue = userContact.val();var userAnswerValue = userAnswer.val();if(!userContentValue || userContentValue == emptyWarning){userContent.val(emptyWarning).addClass('reportBugWarning');permission = false;}if(!userAnswerValue || userAnswerValue == emptyWarning){userAnswer.val(emptyWarning).addClass('reportBugWarning');permission = false;}else if(userAnswerValue != answer){userAnswer.val(answerWarning).addClass('reportBugWarning');permission = false;refreshQuestion();}if(permission && userContentValue.length <= limit){if(!userContactValue){userContactValue = 'BRAK KONTAKTU';}var data = {type : 'postme',option : 'report-bug',userContent : userContentValue,userContact : userContactValue,url : window.location.pathname};var result = $.post('/gp-mechanism', data);var body = $('body');var successMsg = '<samp id="reportBugSuccess">Twoja uwaga dotar≈Ça do celu. Dziƒôkujƒô :)</samp>';var failMsg = '<samp id="reportBugFail">WystƒÖpi≈Ç nieoczekiwany b≈ÇƒÖd po stronie serwera. Spr√≥buj ponownie p√≥≈∫niej :(</samp>';result.done(function(php){structure.parent().parent().remove();if(php == 'success'){body.append(successMsg);}else{body.append(failMsg);}});result.fail(function(php){structure.parent().parent().remove();body.append(failMsg);});}permission = true;});};function showHideContent(h4Button, structure){h4Button.click(function(){if(structure.is(':visible')){structure.hide();h4Button.removeClass('on').text('zg≈Ço≈õ b≈ÇƒÖd lub sugestiƒô');}else{refreshQuestion();structure.show();h4Button.addClass('on').text('zwi≈Ñ');}}).addClass('on').text('zwi≈Ñ');};};GD.markMessage = function(){$('#main [class^="any-mark"]').each(function(){var quarantine = [];var jsTag = '__';var restorePattern;var htmlSource = $(this).html().replace(/<[^>]+>/g, function(token){quarantine.push(token);return jsTag + (quarantine.length - 1) + jsTag;}).replace(/("[^"]*")/g, '<span>$1</span>');var limit = quarantine.length;for(var i = 0; i < limit; i++){restorePattern = new RegExp(jsTag + i + jsTag);htmlSource = htmlSource.replace(restorePattern, quarantine[i]);}$(this).html(htmlSource);});};GD.hiddenExample = function(){var examples = $('[data-js="tabs-mechanism"]');if(examples.length){hideExamples();addNavigation();showHideContent();}function hideExamples(){examples.children().hide();};function addNavigation(){examples.each(function(){var amount = $(this).children().length;var items = '';if(amount == 1){items = '<li>Przyk≈Çad</li>';}else{for(var i = 0; i < amount; i++){items += '<li>Przyk≈Çad - ' + (i + 1) + '</li>';}}$(this).before('<ins class="hiddenExampleNavigation"><ol class="floatfix">' + items + '</ol></ins>');});};function showHideContent(){examples.each(function(){$(this).prev().children().eq(0).children().click(function(){var order = $(this).index();var example = $(this).parent().parent().next().children().eq(order);var liClass = 'on';if(example.is(':visible')){example.hide();$(this).removeClass();}else{example.parent().children(':visible').hide();$(this).parent().children('.' + liClass).removeClass();example.show();$(this).addClass(liClass);}});});};};GD.setContextPlusWindow = function(){$('body').append('<ins id="contextPlusWindow"><aside></aside></ins>');$('#contextPlusWindow').mouseenter(function(){$(this).stop().fadeIn(100);}).mouseleave(function(){$(this).stop().fadeOut(5000);});};GD.contextPlus = function(where){var place = where || $('#main');var aLinks = place.find('[href^="/kurs-html/kategorie/"]:not([href$="zawartosc-tekstowa"])');var contextAsideWindow = $('#contextPlusWindow');if(aLinks.length){addPlusButton();getData();}function addPlusButton(){aLinks.after(' <ins class="contextPlusButton">+</ins>');};function getData(){aLinks.next().one('click', function(){var insButton = $(this);insButton.after(' <ins class="contextPlusStructure"><samp>wczytujƒô...</samp></ins>');var structure = insButton.next();var url = insButton.prev().attr('href');var result = $.get('/gp-mechanism', 'type=getonepart&file=' + url);result.done(function(php){var php = prepareContent(php, insButton);var structureContent = '<span>{ - ' + php[0] + '}</span>' + (php[1] ? '<small>' + php[1] + '</small>' : '');structure.html(structureContent);if(!this.mobile && php[1]){showException(structure);contextPlus(structure);}});result.fail(function(){insButton.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});showHideContent(insButton, structure);});};function prepareContent(php, insButton){var php = $(php);var ol1Items = php.find('#html-ol-list-1 > li');var ol2Items = php.find('#html-ol-list-2 > li');var tags = '';var exceptions = '';var categoryHref = insButton.prev().attr('href');var n = 0;ol1Items.each(function(){if($(this).hasClass('exception-element')){var elementButton = $(this).children(':first');var exceptionButton = $(this).children(':last');elementButton.addClass('contextPlusException').attr('data-index', n++);if(!this.mobile){exceptionButton.remove();}else{elementButton.after('&nbsp;');exceptionButton.attr('href', categoryHref + exceptionButton.attr('href'));}}tags += $(this).html() + ' - ';});if(!this.mobile && ol2Items.length){ol2Items.each(function(){exceptions += '<span>' + $(this).html() + '</span>';});}return [tags, exceptions];};function showException(structure){structure.children(':first').children('.contextPlusException').mouseenter(function(){if(!where){var contentWindow = contextAsideWindow.children(':first');}else{var contentWindow = $(this).parent().parent().parent().parent().children(':last');}contentWindow.html(structure.children(':last').children().eq($(this).attr('data-index')).clone(true)).prepend('<span>' + ($(this).text() == 'TEKST' ? $(this).text() : 'Element "' + $(this).text() + '"') +'</span>').append('<div></div>');if(!where){contextAsideWindow.stop().fadeIn(100);}else{if(contentWindow.is(':hidden')){contentWindow.show();}}}).mouseleave(function(){if(!where){contextAsideWindow.stop().fadeOut(5000);}});};function showHideContent(insButton, structure){insButton.click(function(){if(structure.is(':visible')){if(!where){var contentWindow = contextAsideWindow;}else{var contentWindow = $(this).parent().parent().children(':last');}if(contentWindow.is(':visible')){contentWindow.hide();}structure.hide();insButton.text('+');}else{structure.show();insButton.text('x');}}).text('x');};};GD.loadLinkContent = function(){var link = $('#main [data-extra-content]');if(link.length){link.each(function(){var contentName = $(this).attr('data-extra-content');$(this).attr('data-extra-content', 'true');$(this).attr('data-dfn', contentName);$(this).one('click', function(){$(this).after('<ins><samp>wczytujƒô...</samp></ins>');var place = $(this).next();var dfn = $(this).attr('data-dfn');var result = $.get('/gp-mechanism', 'type=getdefinition&id=' + dfn);result.done(function(php){place.html(php);});result.fail(function(){place.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});setShowHideMechanism($(this));});});}function setShowHideMechanism(link){link.click(function(){var content = $(this).next();if(content.is(':visible')){content.hide();}else{content.show();}});};};GD.loadCSSFunction = function(){$('#opis-wartosci > article > header > h2:contains("()")').each(function(){var divButton;var mainSection = $(this).parent().parent().children('section:first');var functionAddress = mainSection.children('p:first').children('a:first').attr('href');mainSection.append('<ins id="loadCSSFunctionStructure"><div data-url="' + functionAddress + '">wczytaj wiƒôcej informacji o funkcji</div></ins>');divButton = mainSection.children(':last').children(':first');divButton.one('click', function(){var functionPlace = divButton.parent();var url = $(this).attr('data-url');var functionName = url.slice(url.lastIndexOf('/') + 1);functionPlace.html('<samp>wczytujƒô...</samp>');var result = $.get('/gp-mechanism', 'type=getonepart&file=' + url);result.done(function(php){var jqo = $('<div>' + php + '</div>');$('[class^="any-pattern-explain"] a[href^="#"]', jqo).each(function(){$(this).attr('href', $(this).attr('href') + '-' + functionName);});$('[class^="any-pattern-explain"] > dl > dt', jqo).each(function(){$(this).attr('id', $(this).attr('id') + '-' + functionName);});functionPlace.html(jqo.html());});result.fail(function(){functionPlace.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});});});};GD.partTooltip = function(){var placeholder = $('[data-tooltip]:first');var root = $(':root');if(placeholder.length){placeholder.append('<ins id="partTooltipButton" title="wyja≈õnienie dodatkowych oznacze≈Ñ">?</ins>');setButtonMechanism();}function setButtonMechanism(){$('#partTooltipButton').one('click', function(){root.css('overflow', 'hidden');GD.fixedWindow.children(':gt(0)').hide();GD.fixedWindow.show();GD.fixedWindow.append('<ins id="partTooltipStructure"><samp>wczytujƒô...</samp></ins>');var structure = GD.fixedWindow.children(':last');var file = placeholder.attr('data-tooltip');var result = $.get('/gp-mechanism', 'type=getme&dir=tooltip&option=ajax&file=' + file);result.done(function(php){structure.html(php);});result.fail(function(){structure.html('<samp>wystƒÖpi≈Ç b≈ÇƒÖd</samp>');});setShowHideMechanism($(this), structure);});};function setShowHideMechanism(button, structure){button.click(function(){if(structure.is(':hidden')){GD.fixedWindow.children(':gt(0)').hide();structure.show();GD.fixedWindow.show();root.css('overflow', 'hidden');}});};};GD.colorCode = function(where){if(!where) return;var codeBlock = $('[data-listing]', where);var pre;var language;var code;var codeRemoved;var removedIndex;var markLeft = '&1&';var markRight = '&2&';if(codeBlock.length){colorProcess();}function colorProcess(){codeBlock.each(function(){pre = $(this);language = pre.attr('data-listing');code = pre.children().eq(0).html();markOff();if(language == 'all'){codeRemoved = {html:[], css:[], js:[]};removedIndex = 0;colorAll();}else if(language == 'html'){colorHTML();}else if(language == 'css'){colorCSS();}else if(language == 'js'){colorJS();}markOn();pre.children().eq(0).html(code);addLines();});};function colorAll(){codeOff();colorHTML();codeOn();};function colorHTML(){var fragCode = [];var fragIndex = 0;var fragPrefix = 'HTML';code = code.replace(/srcdoc="[^]+?"/g, function(code){fragCode.push(code.replace(/srcdoc="([^]+?)"/, '$1'));return 'srcdoc="__' + fragPrefix + fragIndex++ + '__"';});code = code.replace(/&lt;!--[^]*?--&gt;/g, function(code){fragCode.push('<span class="commentHTMLCode">' + code + '</span>');return '__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/"[^]*?"|'[^]*?'/g, function(code){fragCode.push(code);return '__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/&amp;[^;]+;/g, function(code){fragCode.push('<span class="entitiesHTMLCode">' + code + '</span>');return '__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/(&lt;![^-][^]*?&gt;)/, function(code){fragCode.push('<span class="doctypeHTMLCode">' + code + '</span>');return '__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/&lt;[^]*?&gt;/g, function(code){code = code.replace(/=[\s]*[^\s&]+|=[\s]*&1&[^\s]+&2&/g, function(code){fragCode.push('<span class="attributeValueHTMLCode">' + code.slice(1) + '</span>');return '=__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/[a-zA-Z-]+(?=[\s]*=|&2)/g, function(code){fragCode.push('<span class="attributeNameHTMLCode">' + code + '</span>');return '__' + fragPrefix + fragIndex++ + '__';});code = code.replace(/(\s|1&)[a-z-]+(?=\s|&)/g, function(code){var firstChar = code[0];if(firstChar == '1'){var size = 2;firstChar = '1&';}else{var size = 1;}fragCode.push('<span class="attributeNameHTMLCode">' + code.slice(size) + '</span>');return firstChar + '__' + fragPrefix + fragIndex++ + '__';});return '<span class="tagHTMLCode">' + code + '</span>';});var fragLimit = fragCode.length - 1;for(var i = fragLimit; i >= 0; i--){code = code.replace(new RegExp('__' + fragPrefix + i + '__'), fragCode[i]);}};function colorCSS(){var fragCode = [];var fragIndex = 0;var fragPrefix = 'CSS';var fragStringCode = [];var fragStringIndex = 0;var fragStringPrefix = 'CSS';code = code.replace(/\/\*[^]*?\*\//g, function(code){fragCode.push('<span class="commentCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/"[^]*?"|'[^]*?'/g, function(code){fragStringCode.push(code);return '__' + fragStringPrefix + fragStringIndex++ + '__';});code = code.replace(/@[^;]+?(?=[\s]*{)|@[^;]+;/g, function(code){fragCode.push('<span class="atRuleCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/[^\s{}/][^}/]+?(?=[\s]*{)(?![^}]+}&)|[^\s{}/&12][^}/]+?(?=[\s]*({|&(1|2)&{))/g, function(code){code = code.replace(/\([^)]*\([^]*?\)[^(]*\)|\([^)]+\)/g, function(code){fragCode.push('<span class="pseudovalueCSSCode">' + code.slice(1, code.length - 1) + '</span>');return '(/' + fragPrefix + fragIndex++ + '/)';});code = code.replace(/\[[^\]]*\]/g, function(code){fragCode.push('<span class="attributeCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/::[\w-]+/g, function(code){fragCode.push('<span class="pseudoelementCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/:[\w-]+\([^)]*\)|:[\w-]+/g, function(code){fragCode.push('<span class="pseudoclassCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/(&gt;|\+|~|,)/g, function(code){fragCode.push('<span class="combinatorCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/#[^\s.&/]+/g, function(code){fragCode.push('<span class="idCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/\.[^\s&/]+/g, function(code){fragCode.push('<span class="classCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});return '<span class="selectorCSSCode">' + code + '</span>';});code = code.replace(/{[^{]*?}/g, function(code){code = code.replace(/!important/g, function(code){fragCode.push('<span class="importantCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/:[^]*?(?=;)/g, function(code){fragCode.push('<span class="valueCSSCode">' + code.slice(1) + '</span>');return ':/' + fragPrefix + fragIndex++ + '/';});code = code.replace(/[^;\s/&][^:\s/&]*(?=:|&2)/g, function(code){fragCode.push('<span class="propertyCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});return '<span class="styleCSSCode">' + code + '</span>';});code = code.replace(/{[^}]*{[^]*?}[^{]*}/g, function(code){fragCode.push('<span class="timestyleCSSCode">' + code + '</span>');return '/' + fragPrefix + fragIndex++ + '/';});var fragLimit = fragCode.length - 1;for(var i = fragLimit; i >= 0; i--){code = code.replace(new RegExp('/' + fragPrefix + i + '/'), fragCode[i]);}var fragStringLimit = fragStringCode.length - 1;for(var i = fragStringLimit; i >= 0; i--){code = code.replace(new RegExp('__' + fragStringPrefix + i + '__'), fragStringCode[i]);}};function colorJS(){code = '<span class="JSCode">' + code + '</span>';};function codeOff(){code = code.replace(/&lt;!--[^]*?--&gt;/g, function(code){codeRemoved.html.push(code);return '__htmlCode' + removedIndex++ + '__';});removedIndex = 0;code = code.replace(/&lt;style[^]*?&gt;[^]+?&lt;\/style&gt;/g, function(code){codeRemoved.css.push(code.replace(/&lt;style[^]*?&gt;([^]+?)&lt;\/style&gt;/, '$1'));return code.replace(/(&lt;style[^]*?&gt;)[^]+?(&lt;\/style&gt;)/, '$1' + '__cssCode' + removedIndex++ + '__' + '$2');});removedIndex = 0;code = code.replace(/&lt;script[^]*?&gt;[^]+?&lt;\/script&gt;/g, function(code){codeRemoved.js.push(code.replace(/&lt;script[^]*?&gt;([^]+?)&lt;\/script&gt;/, '$1'));return code.replace(/(&lt;script[^]*?&gt;)[^]+?(&lt;\/script&gt;)/, '$1' + '__jsCode' + removedIndex++ + '__' + '$2');});};function codeOn(){var codeGhost = code;var finalCode = {};if(codeRemoved.html[0]){var removedLimit = codeRemoved.html.length;for(var i = 0; i < removedLimit; i++){var codePattern = '__htmlCode' + i + '__';code = codeRemoved.html[i];if(codeGhost.indexOf(codePattern) > -1){codeGhost = codeGhost.replace(new RegExp('__htmlCode' + i + '__'), '<span class="commentHTMLCode">' + code + '</span>');}else{finalCode[codePattern] = code;}}}if(codeRemoved.css[0]){var removedLimit = codeRemoved.css.length;for(var i = 0; i < removedLimit; i++){var codePattern = '__cssCode' + i + '__';code = codeRemoved.css[i];if(codeGhost.indexOf(codePattern) > -1){colorCSS();codeGhost = codeGhost.replace(new RegExp('__cssCode' + i + '__'), code);}else{finalCode[codePattern] = code;}}}if(codeRemoved.js[0]){var removedLimit = codeRemoved.js.length;for(var i = 0; i < removedLimit; i++){var codePattern = '__jsCode' + i + '__';code = codeRemoved.js[i];if(codeGhost.indexOf(codePattern) > -1){colorJS();codeGhost = codeGhost.replace(new RegExp('__jsCode' + i + '__'), code);}else{finalCode[codePattern] = code;}}}for(var key in finalCode){codeGhost = codeGhost.replace(key, finalCode[key]);}code = codeGhost;};function markOff(){code = code.replace(/<span>/g, markLeft).replace(/<\/span>/g, markRight);};function markOn(mode){code = code.replace(new RegExp(markLeft, 'g'), '<span class="markCode">').replace(new RegExp(markRight, 'g'), '</span>');};function addLines(){var limit = code.match(/^/gm).length;var lines = '';for(var i = 0; i < limit; i++){lines += '<span></span>';}pre.children().eq(0).prepend('<ins>' + lines + '</ins>');};};GD.hashFix = function(){var id = window.location.hash;var target, distance, offset;if(id){target = $(id);if(target.length){distance = target.offset().top;window.scrollTo(0, distance);}}};GD.adsense = function(){var windowWidth = window.innerWidth;var place1 = $('#page-top');var place2 = $('#page-left-side');var place2Width = place2.width() - 6;var contentHeight = $('#main').height();var menuHeight = $('#page-menu-left').height();var heightResult = contentHeight - menuHeight;var nextHeight = 0;var nextAdsense = true;var ads = {        a1 : '<ins class="adsbygoogle" style="display:block;" data-ad-client="ca-pub-4593722958911452" data-ad-slot="9858445524" data-ad-format="auto"></ins>', a2 : '<ins class="adsbygoogle" style="display:block;width:970px;height:250px;" data-ad-client="ca-pub-4593722958911452" data-ad-slot="5428245921"></ins>', a3 : '<ins class="adsbygoogle" style="display:block;width:750px;height:200px;" data-ad-client="ca-pub-4593722958911452" data-ad-slot="6904979125"></ins>',        b1 : '<ins class="adsbygoogle" style="display:block;" data-ad-client="ca-pub-4593722958911452" data-ad-slot="5288645127" data-ad-format="auto"></ins>', b2 : '<ins class="adsbygoogle" style="display:block;width:336px;height:280px;" data-ad-client="ca-pub-4593722958911452" data-ad-slot="9718844721"></ins>', b3 : '<ins class="adsbygoogle" style="display:b